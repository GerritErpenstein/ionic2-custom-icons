{
  "name": "ionic2-custom-icons",
  "version": "0.4.1-beta.1",
  "description": "A npm-script for creating custom icon fonts and Angular 2 directives to render the icons in your Ionic 2 app.",
  "author": {
    "email": "gerrit.erpenstein.dev@gmail.com",
    "name": "Gerrit Erpenstein"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GerritErpenstein/ionic2-custom-icons.git"
  },
  "bugs": {
    "url": "https://github.com/GerritErpenstein/ionic2-custom-icons/issues"
  },
  "bin": {
    "ionic2-custom-icons": "./npm-script/bin/ionic2-custom-icons.js"
  },
  "main": "directive/dist/index.js",
  "scripts": {
    "clean": "rm -rf ionic/dist npm-script/dist",
    "prebuild": "npm run clean && mkdir ionic/dist && mkdir npm-script/dist",
    "directive:build": "ngc -p ionic/tsconfig.json",
    "npm-script:build": "tsc -p npm-script",
    "build": "npm run directive:build && npm run npm-script:build",
    "markdown-toc": "markdown-toc -i README.md"
  },
  "dependencies": {
    "fs-extra": "1.0.0",
    "gulp": "3.9.1",
    "gulp-iconfont": "8.0.1",
    "gulp-rename": "1.2.2",
    "gulp-template": "4.0.0",
    "gulp-util": "3.0.7",
    "lodash": "4.16.6",
    "merge-stream": "1.0.0"
  },
  "devDependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@types/es6-promise": "0.0.32",
    "@types/es6-shim": "^0.31.32",
    "@types/fs-extra": "0.0.33",
    "@types/lodash": "4.14.36",
    "@types/node": "6.0.45",
    "ionic-angular": "2.0.0",
    "markdown-toc": "0.13.0",
    "rxjs": "5.0.0-beta.12",
    "typescript": "2.0.9",
    "zone.js": "0.6.26"
  },
  "keywords": [
    "ionic",
    "ionic2",
    "angular",
    "angular2",
    "mobile",
    "app",
    "hybrid",
    "cordova",
    "icons",
    "custom",
    "font"
  ]
}
